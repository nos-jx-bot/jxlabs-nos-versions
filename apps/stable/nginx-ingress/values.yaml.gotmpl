controller:
  replicaCount: 3
  service:
    omitClusterIP: true
  publishService:
    enabled: true
    pathOverride: nginx-ingress-controller
  {{- if eq .Values.jxRequirements.cluster.provider "kind" }}
    type: NodePort
  {{- end }}
  {{- if eq .Values.jxRequirements.cluster.provider "eks" }}
  annotations:
    service.beta.kubernetes.io/aws-load-balancer-type: nlb
  {{- end }}
defaultBackend:
  service:
    omitClusterIP: true
rbac:
  create: true
